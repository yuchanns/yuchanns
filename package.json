{
  "name": "vuepress-theme-yuchanns",
  "version": "2.0.0",
  "main": "lib/node/index.js",
  "types": "lib/node/index.d.ts",
  "repository": "git@github.com:yuchanns/yuchanns.git",
  "author": "yuchanns <airamusume@gmail.com>",
  "license": "MIT",
  "private": false,
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "tsc -b theme/tsconfig.build.json",
    "clean": "rimraf lib theme/*.tsbuildinfo .vuepress/.{cache,temp} .vuepress/dist",
    "copy": "cpx \"theme/src/**/*.{d.ts,scss,ttf}\" theme/lib",
    "site:dev": "yarn build && yarn copy && vuepress dev",
    "site:build": "yarn build && yarn copy && vuepress build"
  },
  "devDependencies": {
    "sass": "^1.49.0",
    "sass-loader": "^12.4.0",
    "vue": "^3.2.28",
    "@vitejs/plugin-vue-jsx": "^1.3.4",
    "cpx": "^1.5.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.5.5",
    "vuepress": "^2.0.0-beta.35"
  }
}
